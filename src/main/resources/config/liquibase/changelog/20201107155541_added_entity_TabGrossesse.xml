            </column>
            <column name="rapport" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="tab_patient_id" type="bigint">
                <constraints unique="true" nullable="true" uniqueConstraintName="ux_tab_grossesse_tab_patient_id" />
            </column>
            <!-- jhipster-needle-liquibase-add-column - JHipster will add columns here -->
        </createTable>
        <dropDefaultValue tableName="tab_grossesse" columnName="date_consult" columnDataType="datetime"/>
        <dropDefaultValue tableName="tab_grossesse" columnName="date_ovulation" columnDataType="datetime"/>
    </changeSet>

    <changeSet id="20201107155541-1-relations" author="jhipster">

    </changeSet>
