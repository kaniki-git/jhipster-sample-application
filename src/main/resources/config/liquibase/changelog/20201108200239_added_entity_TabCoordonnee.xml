    <!--
        Added the entity TabCoordonnee.
    -->
    <changeSet id="20201108200239-1" author="jhipster">
        <createTable tableName="tab_coordonnee">
            <column name="id" type="bigint">
                <constraints primaryKey="true" nullable="false"/>
            </column>
            <column name="quartier" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="commune" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="ville" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="code_ville" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="rue" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="numero" type="integer">
                <constraints nullable="true" />
            </column>
            <column name="telephone" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="portable" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="prevenir" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="fax" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="adresse_mail" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="adresse_prevenir" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="adresse_libelle_long" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="matriculecreation" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="datecreation" type="datetime">
                <constraints nullable="true" />
            </column>
            <column name="matriculemodif" type="varchar(255)">
                <constraints nullable="true" />
            </column>
            <column name="datemodif" type="datetime">
                <constraints nullable="true" />
            </column>
            <!-- jhipster-needle-liquibase-add-column - JHipster will add columns here -->
        </createTable>
        <dropDefaultValue tableName="tab_coordonnee" columnName="datecreation" columnDataType="datetime"/>
        <dropDefaultValue tableName="tab_coordonnee" columnName="datemodif" columnDataType="datetime"/>
    </changeSet>

    <changeSet id="20201108200239-1-relations" author="jhipster">

    </changeSet>
    <!-- jhipster-needle-liquibase-add-changeset - JHipster will add changesets here -->

