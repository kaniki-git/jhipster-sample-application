                                 baseTableName="tab_rendezvous"
                                 constraintName="fk_tab_rendezvous_tab_personnel_id"
                                 referencedColumnNames="id"
                                 referencedTableName="tab_personnel"/>

    </changeSet>
</databaseChangeLog>
