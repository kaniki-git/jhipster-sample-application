<?xml version="1.0" encoding="utf-8"?>
<databaseChangeLog
    xmlns="http://www.liquibase.org/xml/ns/dbchangelog"
    xmlns:xsi="http://www.w3.org/2001/XMLSchema-instance"
    xsi:schemaLocation="http://www.liquibase.org/xml/ns/dbchangelog http://www.liquibase.org/xml/ns/dbchangelog/dbchangelog-3.9.xsd">
    <!--
        Added the constraints for entity TabRendezvous.
    -->
    <changeSet id="20201115112008-2" author="jhipster">
        
        <addForeignKeyConstraint baseColumnNames="tab_patient_id"
                                 baseTableName="tab_rendezvous"
                                 constraintName="fk_tab_rendezvous_tab_patient_id"
